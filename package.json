{
	"name": "@polar/polar",
	"version": "0.1.0",
	"description": "POLAR core package. Provides POLAR environment with store setup, localization, UI framework, and other basics.",
	"keywords": [
		"OpenLayers",
		"ol",
		"POLAR",
		"core",
		"base"
	],
	"license": "EUPL-1.2",
	"type": "module",
	"author": "Dataport AÃ¶R <polar@dataport.de>",
	"engines": {
		"node": "^22.18.0",
		"npm": "^10.9.3"
	},
	"exports": {
		".": {
			"types": "./dist/polar.d.ts",
			"default": "./dist/polar.js"
		},
		"./polar.css": "./dist/polar.css",
		"./store": {
			"types": "./dist/store.d.ts",
			"default": "./dist/store.js"
		},
		"./plugins/fullscreen": {
			"types": "./dist/plugin-fullscreen.d.ts",
			"default": "./dist/plugin-fullscreen.js"
		},
		"./plugins/fullscreen/store": {
			"types": "./dist/plugin-fullscreen-store.d.ts",
			"default": "./dist/plugin-fullscreen-store.js"
		}
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Dataport/polar.git"
	},
	"scripts": {
		"dev": "vite",
		"snowbox": "VITE_DEV_LANDING=\"examples/snowbox\" vite",
		"preview:build": "vite build --watch -c vite.config.preview.ts",
		"preview:preview": "vite preview -c vite.config.preview.ts",
		"preview": "conc --kill-others 'npm run build' 'npm run preview:build' 'npm run preview:preview'",
		"build": "vite build --watch",
		"build:ci": "vite build",
		"lint": "eslint --cache .",
		"lint:ci": "eslint .",
		"lint:fix": "npm run lint -- --fix",
		"tsc": "tsc && vue-tsc -p src",
		"tsc:ci": "tsc && vue-tsc -p src",
		"docs": "node scripts/typedoc-install-vue-tsc.ts && typedoc --watch",
		"docs:core": "node scripts/typedoc-install-vue-tsc.ts && typedoc --watch --targetAudience core",
		"docs:plugin": "node scripts/typedoc-install-vue-tsc.ts && typedoc --watch --targetAudience plugin",
		"docs:ci": "typedoc",
		"clean": "node scripts/clean.ts",
		"postinstall": "patch-package",
		"test": "vitest",
		"test:ci": "vitest run",
		"test:coverage": "vitest run --coverage",
		"prepare": "node .husky/install.js"
	},
	"files": [
		"dist/**/*",
		"CHANGELOG.md"
	],
	"devDependencies": {
		"@cesium/engine": "^15.0.0",
		"@dataport/eslint-config-geodev": "^0.3.3",
		"@kern-ux/native": "^2.3.0",
		"@masterportal/masterportalapi": "2.48.0",
		"@material-symbols/svg-400": "^0.33.0",
		"@repositoryname/noop": "^1.0.6",
		"@types/node": "^24.2.0",
		"@vitejs/plugin-vue": "^6.0.0",
		"@vue/tsconfig": "^0.7.0",
		"buffer": "^6.0.3",
		"cesium": "^1.131.0",
		"concurrently": "^9.2.0",
		"es-toolkit": "^1.39.8",
		"eslint": "^9.31.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-prettier": "^5.5.3",
		"events": "^3.3.0",
		"hammerjs": "2.0.8",
		"husky": "^9.1.7",
		"i18next": "^25.2.1",
		"i18next-browser-languagedetector": "^8.2.0",
		"i18next-vue": "^5.3.0",
		"jsdom": "^26.1.0",
		"magic-string": "^0.30.17",
		"ol": "10.4.0",
		"patch-package": "^8.0.0",
		"pinia": "^3.0.3",
		"prettier": "^3.6.2",
		"sass": "^1.89.2",
		"stream-browserify": "^3.0.0",
		"timers-browserify": "^2.0.12",
		"tsarch": "^5.4.1",
		"typedoc": "^0.28.8",
		"typedoc-plugin-vue": "^1.5.0",
		"typescript": "~5.8.3",
		"util": "^0.12.5",
		"vite": "^7.0.6",
		"vite-plugin-checker": "^0.10.2",
		"vite-plugin-commonjs": "^0.10.4",
		"vite-plugin-dts": "^4.5.4",
		"vite-plugin-vue-devtools": "^8.0.0",
		"vitest": "^3.2.4",
		"vue": "^3.5.18",
		"vue-tsc": "^3.0.4"
	}
}
