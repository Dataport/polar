{
	"name": "@polar/polar",
	"version": "3.0.0-alpha.1",
	"description": "Configurable map client based on OpenLayers",
	"keywords": [
		"OpenLayers",
		"ol",
		"MasterportalAPI",
		"POLAR"
	],
	"license": "EUPL-1.2",
	"type": "module",
	"author": "Dataport AÃ¶R <polar@dataport.de>",
	"engines": {
		"node": "^22.18.0",
		"npm": "^10.9.3"
	},
	"exports": {
		".": {
			"types": "./dist/polar.d.ts",
			"default": "./dist/polar.js"
		},
		"./polar.css": "./dist/polar.css",
		"./store": {
			"types": "./dist/store.d.ts",
			"default": "./dist/store.js"
		},
		"./plugins/*": {
			"types": "./dist/plugin-*.d.ts",
			"default": "./dist/plugin-*.js"
		},
		"./plugins/*/store": {
			"types": "./dist/plugin-*-store.d.ts",
			"default": "./dist/plugin-*-store.js"
		}
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Dataport/polar.git"
	},
	"scripts": {
		"dev": "vite",
		"snowbox": "cross-env VITE_DEV_LANDING=examples/snowbox vite",
		"preview:build": "vite build --watch -c vite.config.preview.ts",
		"preview:preview": "vite preview -c vite.config.preview.ts",
		"preview": "conc --kill-others 'npm run build' 'npm run preview:build' 'npm run preview:preview'",
		"build": "vite build --watch",
		"build:ci": "vite build",
		"lint": "eslint --cache .",
		"lint:ci": "eslint .",
		"lint:fix": "npm run lint -- --fix",
		"tsc": "tsc && vue-tsc -p src",
		"tsc:ci": "tsc && vue-tsc -p src",
		"docs:ci": "npm run docs:showcase:ci && npm run docs:docs:ci && npm run docs:api:ci && npm run docs:api:core:ci && npm run docs:api:plugin:ci",
		"docs:showcase:ci": "mkdir -p docs-html && cp -r examples/github-io/* docs-html/",
		"docs:docs": "mkdocs serve",
		"docs:docs:ci": "mkdocs build",
		"docs:api": "node scripts/typedoc-install-vue-tsc.ts && typedoc --watch",
		"docs:api:core": "node scripts/typedoc-install-vue-tsc.ts && typedoc --watch --targetAudience core",
		"docs:api:plugin": "node scripts/typedoc-install-vue-tsc.ts && typedoc --watch --targetAudience plugin",
		"docs:api:ci": "typedoc",
		"docs:api:core:ci": "typedoc --targetAudience core --out docs-html/reference-core",
		"docs:api:plugin:ci": "typedoc --targetAudience plugin --out docs-html/reference-plugin",
		"clean": "node scripts/clean.ts",
		"postinstall": "patch-package",
		"test": "vitest",
		"test:ci": "vitest run",
		"test:coverage": "vitest run --coverage",
		"prepare": "node .husky/install.js"
	},
	"files": [
		"dist/**/*",
		"README.md",
		"CHANGELOG.md",
		"security.md"
	],
	"devDependencies": {
		"@actions/github": "^6.0.1",
		"@commitlint/cli": "^19.8.1",
		"@commitlint/config-conventional": "^19.8.1",
		"@dataport/eslint-config-geodev": "^0.4.0",
		"@kern-ux/native": "^2.3.0",
		"@masterportal/masterportalapi": "2.48.0",
		"@material-symbols/svg-400": "^0.35.0",
		"@pinia/testing": "^1.0.2",
		"@repositoryname/noop": "^1.0.6",
		"@types/node": "^24.3.1",
		"@vitejs/plugin-vue": "^6.0.1",
		"@vitest/coverage-v8": "^3.2.4",
		"@vue/test-utils": "^2.4.6",
		"@vue/tsconfig": "^0.8.1",
		"buffer": "^6.0.3",
		"concurrently": "^9.2.1",
		"conventional-changelog": "^7.1.1",
		"conventional-changelog-conventionalcommits": "^9.1.0",
		"cross-env": "^10.0.0",
		"es-toolkit": "^1.39.10",
		"eslint": "^9.34.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-perfectionist": "^4.15.0",
		"eslint-plugin-prettier": "^5.5.4",
		"events": "^3.3.0",
		"hammerjs": "2.0.8",
		"husky": "^9.1.7",
		"i18next": "^25.5.0",
		"i18next-browser-languagedetector": "^8.2.0",
		"i18next-vue": "^5.3.0",
		"jsdom": "^26.1.0",
		"magic-string": "^0.30.18",
		"ol": "10.4.0",
		"patch-package": "^8.0.0",
		"pinia": "^3.0.3",
		"prettier": "^3.6.2",
		"stream-browserify": "^3.0.0",
		"timers-browserify": "^2.0.12",
		"tsarch": "^5.4.1",
		"typedoc": "^0.28.12",
		"typedoc-plugin-vue": "^1.5.0",
		"typescript": "~5.9.2",
		"util": "^0.12.5",
		"vite": "^7.1.5",
		"vite-plugin-checker": "^0.10.3",
		"vite-plugin-commonjs": "^0.10.4",
		"vite-plugin-dts": "^4.5.4",
		"vite-plugin-kern-extra-icons": "^0.3.0",
		"vite-plugin-vue-devtools": "^8.0.1",
		"vitest": "^3.2.4",
		"vue": "^3.5.21",
		"vue-tsc": "^3.0.6"
	}
}
